                    "StartAt": "DetectLandmark",
                    "States": {
                      "DetectLandmark": {
                        "Type": "Task",
                        "Resource": "arn:aws:lambda:#{AWS::Region}:#{AWS::AccountId}:function:chehara-dev-detectLandmark",
                        "Comment": "Call Google Vision API to detect landmarks in the given image",
                        "TimeoutSeconds": 60,
                        "End": true
                      }
                    }
                  },
                  {
                    "StartAt": "DetectText",
                    "States": {
                      "DetectText": {
                        "Type": "Task",
